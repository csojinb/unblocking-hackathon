{
	"manifest_version": 2,
	"name": "Gender Analyzer",
  	"description": "Will highlight all the names on a page and estimate which gender they belong to.",
  	"version": "0.1",
	"permissions": ["tabs"],
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"js": ["content.js"],
			"css": ["content.css"]
		}
	],
	"browser_action": {
		"default_icon": "icon.png",
   		"default_popup": "popup.html",
		"default_title": "Who Said What?"
	}
	//"options_page": "options.html"
}
